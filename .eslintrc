{
   "parser": "babel-eslint",
   "extends": "airbnb",
   "env": {
     "browser": true,
     "node": true,
     "mocha": true,
     "es6": true
   },
   "rules": {
     "react/jsx-no-bind": 0,
     "no-constant-condition": 0,
     "new-cap": 0,
     "default-case": 0,
     "react/jsx-first-prop-new-line": 0,
     "import/no-unresolved": 0,
     "newline-per-chained-call": 0,
     "global-require": 0,
     "class-methods-use-this": 0,
     "react/forbid-prop-types": 0,
     "jsx-a11y/no-static-element-interactions": 0,
     "react/sort-comp": 0
   }
 }